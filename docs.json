{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "almond",
  "name": "Neptureon",
  "colors": {
    "primary": "#00D3F2",
    "light": "#34e1f7",
    "dark": "#00b8d9"
  },
  "favicon": "/favicon.png",
  "fonts": {
    "heading": {
      "family": "Manrope",
      "source": "/fonts/Manrope-Bold.woff2",
      "format": "woff2",
      "weight": 700
    },
    "body": {
      "family": "Manrope",
      "source": "/fonts/Manrope-Regular.woff2",
      "format": "woff2",
      "weight": 400
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "getting-started/sign-up",
              "getting-started/explore-main-feed",
              "getting-started/customize-trackers",
              "getting-started/history"
            ]
          },
          {
            "group": "Token Creation",
            "pages": [
              "token-creation/introduction",
              "token-creation/wallets",
              "token-creation/deployer-mode",
              "token-creation/deploy-panel"
            ]
          },
          {
            "group": "Resources",
            "pages": [
              "brand-resources"
            ]
          },
          {
            "group": "Legal",
            "pages": [
              "terms-of-service",
              "privacy-policy"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Discord",
          "href": "https://discord.gg/neptureon",
          "icon": "discord"
        },
        {
          "anchor": "Twitter",
          "href": "https://twitter.com/neptureon",
          "icon": "x-twitter"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/fullLogo.svg",
    "dark": "/logo/fullLogo.svg",
    "height": 40
  },
  "navbar": {
    "links": [
      {
        "label": "App",
        "href": "https://neptureon.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Launch Neptureon",
      "href": "https://neptureon.com"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/neptureon",
      "discord": "https://discord.gg/neptureon"
    }
  },
  "background": {
    "style": "gradient",
    "color": {
      "dark": "#050810"
    }
  },
  "modeToggle": {
    "default": "dark"
  }
}
